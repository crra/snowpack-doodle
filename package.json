{
  "name": "snowpack-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "preact": "^10.2.1",
    "wouter": "^2.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/preset-typescript": "^7.8.3",
    "babel-plugin-minify-builtins": "^0.5.0",
    "babel-plugin-minify-constant-folding": "^0.5.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-minify-mangle-names": "^0.5.0",
    "babel-plugin-minify-type-constructors": "^0.4.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-consecutive-adds": "^0.4.3",
    "babel-plugin-transform-merge-sibling-variables": "^6.9.4",
    "babel-plugin-transform-minify-booleans": "^6.9.4",
    "babel-plugin-transform-regexp-constructors": "^0.4.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "babel-plugin-transform-remove-undefined": "^0.5.0",
    "babel-preset-minify": "^0.5.1",
    "concurrently": "^5.0.2",
    "cpx-fixed": "^1.6.0",
    "lite-server": "^2.5.4",
    "prettier": "^1.19.1",
    "snowpack": "^1.0.4",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "clean:prod": "node shell/remove.js \"./prod\"",
    "clean:dev": "node shell/remove.js \"./dist\"",
    "build": "npm run build:js && npm run build:esm && npm run copy:static",
    "build:js": "babel --extensions \".ts,.tsx,.js,.jsx\" src/ -d dist",
    "build:js:prod": "BABEL_ENV=production babel --extensions \".ts,.tsx\" src/ -d dist",
    "build:js:watch": "babel --extensions \".ts,.tsx,.js,.jsx\" src/ -d dist --watch",
    "build:esm:prod": "snowpack --optimize --no-source-map --include \"dist/**/*.js\" --dest dist/web_modules",
    "build:esm": "snowpack --include \"dist/**/*.js\" --dest dist/web_modules",
    "copy:static": "cpx-fixed \"src/*.{html,css}\" ./dist",
    "copy:prod": "node shell/rename.js ./dist ./prod",
    "start-watch": "npm run clean:dev && npm run build && npm run build:js:watch",
    "server": "lite-server",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run server\"",
    "prod": "npm run clean:dev && npm run clean:prod && npm run build:js:prod && npm run build:esm:prod && npm run copy:prod"
  },
  "keywords": [
    "snowpack",
    "typescript",
    "preact",
    "router",
    "babel",
    "css grid",
    "JSX",
    "Fragment"
  ],
  "author": "crra",
  "license": "MIT"
}
