{
  "name": "snowpack-doodle",
  "version": "1.0.0",
  "description": "Sample project with: Preact, JSX, Fragment, Router, CSS Grid, Typescript, Babel",
  "main": "index.js",
  "dependencies": {
    "preact": "^10.3.2",
    "wouter": "^2.4.0"
  },
  "snowpack": {
    "webDependencies": [
      "preact/compat",
      "wouter/preact"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/preset-typescript": "^7.8.3",
    "babel-plugin-add-import-extension": "^1.3.0",
    "babel-plugin-minify-builtins": "^0.5.0",
    "babel-plugin-minify-constant-folding": "^0.5.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-minify-mangle-names": "^0.5.0",
    "babel-plugin-minify-type-constructors": "^0.4.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-consecutive-adds": "^0.4.3",
    "babel-plugin-transform-merge-sibling-variables": "^6.9.4",
    "babel-plugin-transform-minify-booleans": "^6.9.4",
    "babel-plugin-transform-regexp-constructors": "^0.4.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "babel-plugin-transform-remove-undefined": "^0.5.0",
    "babel-preset-minify": "^0.5.1",
    "cpx-fixed": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "servor": "^3.2.0",
    "snowpack": "^1.3.1",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "build": "npm-run-all --sequential build:clean build:dependencies build:ts build:move_modules build:copy:assets build:rename",
    "build:clean": "node shell/remove.js \"./build\"",
    "build:dependencies": "snowpack --babel --optimize --clean --no-source-map --include \"src/**/*.{ts,tsx,.js,jsx}\"",
    "build:ts": "set BABEL_ENV=production&&babel src --out-dir build --extensions \".ts,.tsx,.js,.jsx\"",
    "build:copy:assets": "cpx-fixed \"src/*.{html,css}\" ./build",
    "build:move_modules": "node shell/rename.js \"web_modules\" \"build/web_modules\"",
    "build:rename": "node shell/rename.js \"build\" \"dist\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "snowpack",
    "typescript",
    "preact",
    "router",
    "babel",
    "css grid",
    "JSX",
    "Fragment"
  ],
  "author": "crra",
  "license": "MIT"
}